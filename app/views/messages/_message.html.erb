<tr id="<%= dom_id message%>">
    <td><%= message.created_at.to_s %>: <%= message.content %></td>
    <td>
      <%= link_to 'Edit', edit_room_message_path(message.room_id, message.id) %>
      <%= button_to "Delete", room_message_path(message.room_id, message.id), method: :delete %>
    </td>

</tr>